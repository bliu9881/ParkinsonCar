# Extreme memory optimization for production builds
NODE_OPTIONS=--max-old-space-size=8192 --optimize-for-size --gc-interval=50 --max-semi-space-size=128
NEXT_TELEMETRY_DISABLED=1
DISABLE_ESLINT_PLUGIN=true
# Disable Next.js build cache in CI to save memory
NEXT_CACHE_DISABLED=1
# Disable source maps to save memory
GENERATE_SOURCEMAP=false